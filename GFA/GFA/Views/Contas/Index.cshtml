@model IEnumerable<GFA.Models.Conta>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.desc_conta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.tipo_conta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.obs_conta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.inativo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.desc_conta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tipo_conta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.obs_conta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.inativo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
